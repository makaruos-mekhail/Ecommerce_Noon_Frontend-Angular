u0631\u0649 \u0641\u064A \u0627\u0644\u0625\u0645\u0627\u0631\u0627\u062A - \u0627\u0644\u0637\u0631\u064A\u0642\u0629 \u0627\u0644\u0623\u0641\u0636\u0644 \u0644\u0644\u062D\u0635\u0648\u0644 \u0639\u0644\u0649 \u0627\u0644\u0641\u0648\u0627\u0643\u0647 \u0648\u0627\u0644\u062E\u0636\u0631\u0627\u0648\u0627\u062A \u0648\u0627\u0644\u0623\u0637\u0639\u0645\u0629 \u0627\u0644\u0645\u0639\u0644\u0628\u0629 \u0648\u0627\u0644\u0641\u0648\u0627\u0643\u0647 \u0627\u0644\u0645\u062C\u0641\u0641\u0629 \u0648\u0627\u0644\u0645\u062E\u0628\u0648\u0632\u0627\u062A \u0648\u0645\u0646\u062A\u062C\u0627\u062A \u0627\u0644\u062D\u0644\u064A\u0628 \u0648\u0627\u0644\u0628\u064A\u0636 \u0648\u0627\u0644\u0645\u0634\u0631\u0648\u0628\u0627\u062A \u0648\u0643\u0644 \u0645\u0627 \u062A\u062D\u062A\u0627\u062C\u0647 \u0644\u0625\u0639\u062F\u0627\u062F \u0648\u062C\u0628\u0627\u062A \u0634\u0647\u064A\u0629 \u0641\u064A \u0634\u0647\u0631 \u0631\u0645\u0636\u0627\u0646. \u0627\u0637\u0644\u0628 \u062C\u0645\u064A\u0639 \u0639\u0646\u0627\u0635\u0631 \u0642\u0627\u0626\u0645\u0629 \u062A\u0633\u0648\u0642\u0643 \u0644\u0634\u0647\u0631 \u0631\u0645\u0636\u0627\u0646 \u0623\u0648\u0646\u0644\u0627\u064A\u0646 \u0628\u0633\u0647\u0648\u0644\u0629 \u0648\u0633\u0631\u0639\u0629 \u0644\u062A\u0635\u0644\u0643 \u062C\u0645\u064A\u0639 \u0645\u0633\u062A\u0644\u0632\u0645\u0627\u062A \u0627\u0644\u0628\u0642\u0627\u0644\u0629 \u0627\u0644\u062A\u064A \u062A\u062D\u062A\u0627\u062C\u0647\u0627 \u062D\u062A\u0649 \u0628\u0627\u0628 \u0645\u0646\u0632\u0644\u0643 \u0641\u064A \u0623\u064A \u0645\u0643\u0627\u0646 \u0641\u064A \u062F\u0648\u0644\u0629 \u0627\u0644\u0625\u0645\u0627\u0631\u0627\u062A. \u0648\u0641\u0651\u0631 \u0648\u0642\u062A\u0643 \u0648\u0631\u0643\u0651\u0632 \u0645\u062C\u0647\u0648\u062F\u0643 \u0639\u0644\u0649 \u0625\u0639\u062F\u0627\u062F \u0627\u0644\u0637\u0639\u0627\u0645 \u0628\u062F\u0644\u064B\u0627 \u0645\u0646 \u0625\u0636\u0627\u0639\u0629 \u0627\u0644\u0648\u0642\u062A \u0641\u064A \u0627\u0644\u0627\u0635\u0637\u0641\u0627\u0641 \u0641\u064A \u0627\u0644\u0637\u0648\u0627\u0628\u064A\u0631.",topic_four_heading_uae:"\u062A\u0633\u0648\u0642 \u0632\u064A\u0646\u0629 \u0648\u062F\u064A\u0643\u0648\u0631\u0627\u062A \u0631\u0645\u0636\u0627\u0646 \u0623\u0648\u0646\u0644\u0627\u064A\u0646 \u0645\u0646 \u0646\u0648\u0646",topic_four_uae:"\u062C\u0647\u0651\u0632 \u0645\u0646\u0632\u0644\u0643 \u0644\u0627\u0633\u062A\u0642\u0628\u0627\u0644 \u0631\u0645\u0636\u0627\u0646 \u0648\u0632\u064A\u0651\u0646\u0647 \u0628\u062F\u064A\u0643\u0648\u0631\u0627\u062A \u0631\u0645\u0636\u0627\u0646 \u0627\u0644\u062A\u064A \u062A\u062C\u062F\u0647\u0627 \u0641\u064A \u0645\u062A\u062C\u0631 \u0646\u0648\u0646 \u0628\u0623\u0634\u0643\u0627\u0644 \u0631\u0627\u0642\u064A\u0629 \u0648\u0645\u0645\u064A\u0632\u0629. \u0646\u0642\u062F\u0645 \u0644\u0643 \u062C\u0645\u064A\u0639 \u062F\u064A\u0643\u0648\u0631\u0627\u062A \u0631\u0645\u0636\u0627\u0646 \u0627\u0644\u0645\u0646\u0632\u0644\u064A\u0629 \u0627\u0644\u062A\u064A \u062A\u062D\u062A\u0627\u062C\u0647\u0627 \u0645\u0646 \u0645\u0643\u0627\u0646 \u0648\u0627\u062D\u062F \u0627\u0628\u062A\u062F\u0627\u0621 \u0645\u0646 \u0627\u0644\u0625\u0636\u0627\u0621\u0627\u062A \u0628\u0634\u0643\u0644 \u0627\u0644\u0647\u0644\u0627\u0644 \u0648\u063A\u064A\u0631\u0647 \u0645\u0646 \u0627\u0644\u0623\u0634\u0643\u0627\u0644 \u0627\u0644\u062C\u0630\u0627\u0628\u0629 \u0648\u062D\u062A\u0649 \u0627\u0644\u0641\u0648\u0627\u0646\u064A\u0633 \u0648\u0645\u0641\u0627\u0631\u0634 \u0627\u0644\u0645\u0627\u0626\u062F\u0629 \u0648\u0635\u0648\u0627\u0646\u064A \u0627\u0644\u062D\u0644\u0648\u064A\u0627\u062A \u0628\u0623\u0634\u0643\u0627\u0644 \u0647\u0644\u0627\u0644 \u0631\u0645\u0636\u0627\u0646 \u0648\u0627\u0644\u0648\u0633\u0627\u0626\u062F \u0648\u062D\u0648\u0627\u0645\u0644 \u0627\u0644\u0634\u0645\u0648\u0639 \u0648\u0632\u064A\u0646\u0629 \u0627\u0644\u0627\u062D\u062A\u0641\u0627\u0644 \u0648\u0623\u0637\u0628\u0627\u0642 \u0627\u0644\u062A\u0642\u062F\u064A\u0645 \u0648\u0627\u0644\u062F\u064A\u0643\u0648\u0631\u0627\u062A \u0627\u0644\u0645\u0632\u062E\u0631\u0641\u0629 \u0648\u062F\u064A\u0643\u0648\u0631\u0627\u062A \u0631\u0645\u0636\u0627\u0646 \u0627\u0644\u0625\u0645\u0627\u0631\u0627\u062A \u0648\u0645\u062C\u0645\u0648\u0639\u0629 \u0645\u0646 \u0627\u0644\u062F\u064A\u0643\u0648\u0631\u0627\u062A \u0627\u0644\u062E\u0627\u0631\u062C\u064A\u0629 \u0627\u0644\u0623\u0646\u064A\u0642\u0629 \u0648\u0627\u0644\u0643\u062B\u064A\u0631 \u0645\u0646 \u0623\u0641\u0643\u0627\u0631 \u0632\u064A\u0646\u0629 \u0631\u0645\u0636\u0627\u0646 \u0627\u0644\u062C\u0630\u0627\u0628\u0629. \u0643\u0645\u0627 \u064A\u062A\u0636\u0645\u0646 \u0642\u0633\u0645 \u0627\u0644\u0623\u062F\u0648\u0627\u062A \u0627\u0644\u0645\u0646\u0632\u0644\u064A\u0629 \u0623\u062F\u0648\u0627\u062A \u0627\u0644\u0637\u0639\u0627\u0645 \u0648\u0623\u0637\u0642\u0645 \u0623\u062F\u0648\u0627\u062A \u0627\u0644\u0645\u0627\u0626\u062F\u0629 \u0648\u0645\u0641\u0627\u0631\u0634 \u0627\u0644\u0645\u0627\u0626\u062F\u0629 \u0648\u0627\u0644\u0645\u0646\u062A\u062C\u0627\u062A \u0627\u0644\u0623\u062E\u0631\u0649 \u0627\u0644\u062A\u064A \u062A\u062D\u062A\u0627\u062C\u0647\u0627 \u0644\u062A\u0646\u0627\u0648\u0644 \u0627\u0644\u0637\u0639\u0627\u0645. \u062A\u0633\u0648\u0642 \u062C\u0645\u064A\u0639 \u0623\u0634\u0643\u0627\u0644 \u0632\u064A\u0646\u0629 \u0631\u0645\u0636\u0627\u0646 \u0627\u0648\u0646\u0644\u0627\u064A\u0646 \u0641\u064A \u0627\u0644\u0625\u0645\u0627\u0631\u0627\u062A \u0644\u062A\u0647\u064A\u0626\u0629 \u0645\u0646\u0632\u0644\u0643 \u0644\u0627\u0633\u062A\u0642\u0628\u0627\u0644 \u0636\u064A\u0648\u0641\u0643 \u0648\u0623\u0642\u0631\u0628\u0627\u0626\u0643.",topic_five_heading_uae:"\u062A\u0633\u0648\u0642 \u0645\u0644\u0627\u0628\u0633 \u0631\u0645\u0636\u0627\u0646 \u0623\u0648\u0646\u0644\u0627\u064A\u0646",topic_five_uae:"\u062A\u0642\u062F\u0645 \u0646\u0648\u0646 \u062A\u0634\u0643\u064A\u0644\u0629 \u0648\u0627\u0633\u0639\u0629 \u0645\u0646 \u0623\u0632\u064A\u0627\u0621 \u0631\u0645\u0636\u0627\u0646 \u0644\u0644\u0631\u062C\u0627\u0644 \u0648\u0627\u0644\u0646\u0633\u0627\u0621 \u0648\u0627\u0644\u0623\u0637\u0641\u0627\u0644. \u0627\u062E\u062A\u0631 \u0627\u0644\u0622\u0646 \u0633\u062A\u0627\u064A\u0644\u0627\u062A\u0643 \u0627\u0644\u0645\u0641\u0636\u0644\u0629 \u062E\u0644\u0627\u0644 \u0639\u0631\u0648\u0636 \u0631\u0645\u0636\u0627\u0646 \u0648\u0639\u0631\u0648\u0636 \u0627\u0644\u0639\u064A\u062F \u0641\u064A \u0627\u0644\u0625\u0645\u0627\u0631\u0627\u062A \u0648\u062A\u0623\u0644\u0642 \u0628\u0625\u0637\u0644\u0627\u0644\u0629 \u0623\u0646\u064A\u0642\u0629 \u0641\u064A \u0647\u0630\u0627 \u0627\u0644\u0639\u064A\u062F. \u062A\u062A\u0636\u0645\u0646 \u062A\u0634\u0643\u064A\u0644\u0629 \u0623\u0632\u064A\u0627\u0621 \u0631\u0645\u0636\u0627\u0646 \u0627\u0644\u0625\u0645\u0627\u0631\u0627\u062A \u0627\u0644\u0645\u0644\u0627\u0628\u0633 \u0627\u0644\u062A\u0631\u0627\u062B\u064A\u0629 \u0648\u0623\u062B\u0648\u0627\u0628 \u0627\u0644\u0635\u0644\u0627\u0629 \u0644\u0644\u0631\u062C\u0627\u0644 \u0648\u0627\u0644\u0646\u0633\u0627\u0621. \u0627\u0633\u062A\u0639\u0631\u0636\u064A \u0645\u062C\u0645\u0648\u0639\u0629 \u0642\u0641\u0637\u0627\u0646\u0627\u062A \u0631\u0645\u0636\u0627\u0646 \u0648\u0627\u062E\u062A\u0627\u0631\u064A \u0645\u0627 \u064A\u0646\u0627\u0633\u0628\u0643 \u0645\u0646 \u0645\u0644\u0627\u0628\u0633 \u0631\u0645\u0636\u0627\u0646 \u0648\u0627\u0644\u0639\u0628\u0627\u064A\u0627\u062A \u0648\u0627\u0644\u0643\u062B\u064A\u0631 \u0645\u0646 \u0627\u0644\u0623\u0632\u064A\u0627\u0621 \u0627\u0644\u0646\u0633\u0627\u0626\u064A\u0629. \u0627\u062E\u062A\u0631 \u0645\u0627 \u062A\u0641\u0636\u0644\u0647 \u0645\u0646 \u0623\u0632\u064A\u0627\u0621 \u0631\u0645\u0636\u0627\u0646 \u0627\u0644\u0631\u062C\u0627\u0644\u064A\u0629\u060C \u0648\u0623\u0643\u0645\u0644 \u0625\u0637\u0644\u0627\u0644\u062A\u0643 \u0628\u0627\u0643\u0633\u0633\u0648\u0627\u0631\u0627\u062A \u0631\u0645\u0636\u0627\u0646 \u0644\u0644\u0631\u062C\u0627\u0644 \u0648\u0627\u0644\u0646\u0633\u0627\u0621 \u0648\u062A\u0634\u0645\u0644 \u0627\u0644\u062D\u0642\u0627\u0626\u0628 \u0648\u0627\u0644\u0623\u0632\u064A\u0627\u0621 \u0648\u0627\u0644\u0646\u0638\u0627\u0631\u0627\u062A \u0648\u0627\u0644\u0633\u0627\u0639\u0627\u062A \u0648\u0627\u0644\u0645\u062C\u0648\u0647\u0631\u0627\u062A \u0648\u063A\u064A\u0631\u0647\u0627 \u0645\u0646 \u0627\u0644\u0644\u0645\u0633\u0627\u062A \u0627\u0644\u0623\u0646\u064A\u0642\u0629 \u0645\u0642\u062F\u0645\u0629 \u0645\u0646 \u0623\u0641\u0636\u0644 \u0627\u0644\u0645\u0627\u0631\u0643\u0627\u062A \u0627\u0644\u0639\u0627\u0644\u0645\u064A\u0629 \u0645\u062B\u0644 \u0623\u062F\u064A\u062F\u0627\u0633 \u0648\u0646\u0627\u064A\u0643 \u0648\u0641\u064A\u0631\u0648 \u0645\u0648\u062F\u0627 \u0648\u0643\u0627\u0633\u064A\u0648 \u0648\u0641\u0648\u0633\u064A\u0644 \u0648\u062A\u0648\u0645\u064A \u0647\u064A\u0644\u0641\u064A\u063A\u0631.",topic_six_heading_uae:"\u0627\u0646\u062A\u0647\u0632 \u0641\u0631\u0635\u0629 \u062A\u062E\u0641\u064A\u0636\u0627\u062A \u0631\u0645\u0636\u0627\u0646 \u0648\u0639\u0631\u0648\u0636 \u0627\u0644\u0639\u064A\u062F \u0648\u0635\u0641\u0642\u0627\u062A \u0646\u0648\u0646 \u0627\u0644\u0645\u0630\u0647\u0644\u0629 \u0627\u0644\u0623\u062E\u0631\u0649",topic_six_uae:"\u062A\u0642\u062F\u0645 \u0644\u0643 \u0646\u0648\u0646 \u0623\u0631\u0648\u0639 \u0627\u0644\u0635\u0641\u0642\u0627\u062A \u0627\u0644\u062D\u0635\u0631\u064A\u0629 \u0641\u064A \u0647\u0630\u0627 \u0627\u0644\u0634\u0647\u0631 \u0627\u0644\u0641\u0636\u064A\u0644 \u0644\u062A\u062A\u0633\u0648\u0642 \u0627\u0644\u0647\u062F\u0627\u064A\u0627 \u0648\u0623\u0635\u0646\u0627\u0641 \u0627\u0644\u0637\u0639\u0627\u0645 \u0648\u0627\u0644\u0623\u0632\u064A\u0627\u0621 \u0648\u0627\u0644\u062F\u064A\u0643\u0648\u0631\u0627\u062A \u0648\u063A\u064A\